<div class="container dashboard-search">
  <%= form_tag users_path, method: :get do %>
  <div class="form-group">
    <%= text_field_tag :search, params[:search], class: "form-control", placeholder: 'Search Names, Emails, By Gender' %>
  </div>
    <%= submit_tag 'Search', :name => nil, class: 'btn btn-success' %>
  <% end %>
</div>